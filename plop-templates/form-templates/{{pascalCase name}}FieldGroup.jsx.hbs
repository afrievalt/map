import React, {memo} from 'react'
{{#each uniqueFields}}
import {{.}}Field from '../../../form/{{.}}.field'
{{/each}}
const {{pascalCase name}}Form = () => {
  return (
    <>
      {{#each field}}
        {{#if string}}
          <InputField fieldId='{{id}}' label='{{titleCase id}}' />
        {{/if}}
        {{#if number}}
          <InputField  type='number' fieldId='{{id}}' label='{{titleCase id}}' />
        {{/if}}
        {{#if select}}
          <InputField  type='number' fieldId='{{id}}' label='{{titleCase id}}' />
        {{/if}}
      {{/each}}
    </>
  )
}

export default memo({{pascalCase name}}Form)