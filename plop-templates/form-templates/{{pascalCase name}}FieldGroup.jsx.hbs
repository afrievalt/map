import React, {memo} from 'react'
{{#each uniqueFields}}
import {{.}}Field from '../../../form/{{.}}.field'
{{/each}}
const {{pascalCase name}}Form = () => {
  return (
    <>
      {{#each field}}
        {{#if string}}
          <InputField fieldId='{{id}}' label='{{titleCase id}}' />
        {{/if}}
        {{#if number}}
          <InputField fieldId='{{id}}' label='{{titleCase id}}' />
        {{/if}}
        {{#if select}}
          <SelectField fieldId='{{id}}' label='{{titleCase id}}'>
            <SelectField.Options data={['One', 'Two', 'Three']} />
          </SelectField>
        {{/if}}
        {{#if bool}}
          <CheckboxField fieldId='{{id}}' label='{{titleCase id}}' />
        {{/if}}
        {{#if date}}
          <DatePickerField fieldId='{{id}}' label='{{titleCase id}}' />
        {{/if}}
      {{/each}}
    </>
  )
}

export default memo({{pascalCase name}}Form)